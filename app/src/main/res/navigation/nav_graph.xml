<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/nav_graph"    app:startDestination="@id/transactionsFragment">    <fragment        android:id="@+id/transactionsFragment"        android:name="com.example.moneytracker.ui.fragments.TransactionsFragment"        tools:layout="@layout/fragment_transactions">        <action            android:id="@+id/action_transactionsFragment_to_addTransactionFragment"            app:destination="@id/addTransactionFragment" />    </fragment>    <fragment        android:id="@+id/addTransactionFragment"        android:name="com.example.moneytracker.ui.fragments.AddTransactionFragment"        tools:layout="@layout/fragment_add_transaction" >        <argument            android:name="transactionId"            app:argType="integer"            android:defaultValue="-1" />    </fragment>    <fragment        android:id="@+id/categoriesFragment"        android:name="com.example.moneytracker.ui.fragments.CategoriesFragment"        tools:layout="@layout/fragment_categories">        <action            android:id="@+id/action_categoriesFragment_to_addEditCategoryFragment"            app:destination="@id/addEditCategoryFragment" />    </fragment>    <fragment        android:id="@+id/addEditCategoryFragment"        android:name="com.example.moneytracker.ui.fragments.AddEditCategoryFragment"        tools:layout="@layout/fragment_add_edit_category">        <argument            android:name="categoryId"            app:argType="integer"            android:defaultValue="-1" />    </fragment>    <action        android:id="@+id/action_global_to_addTransactionFragment"        app:destination="@id/addTransactionFragment"/>    <fragment        android:id="@+id/reportsFragment"        android:name="com.example.moneytracker.ui.fragments.ReportsFragment"        tools:layout="@layout/fragment_reports" />    <fragment        android:id="@+id/accountsFragment"        android:name="com.example.moneytracker.ui.fragments.AccountsFragment"        tools:layout="@layout/fragment_accounts" >    </fragment></navigation>